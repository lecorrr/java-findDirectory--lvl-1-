package sample;

import java.io.File;

public class Main {

	public static void main(String[] args) {
		File mainFolder = new File(".");
		File[] files = mainFolder.listFiles();
		for (int i = 0; i < files.length; i++) {
			if(files[i].isDirectory()) {
				System.out.println(files[i]);
			}
		}
	}

}
